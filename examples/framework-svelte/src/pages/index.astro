---
import Component from '../components/Component.astro';

const components = [
	{
		id: 'section1-1',
		component: 'section1'
	},
	{
		id: 'section1-3',
		component: 'section1',
		props: {
			text: 'Section 3'
		}
	},
	{
		id: 'section1-2',
		component: 'section1',
		props: {
			text: 'Section 2'
		}
	}
];
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>
		{components.map((component) => <Component {...component} />)}
	</body>
</html>
